<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="repository.yellowdragon.libreelec.emby" name="LibreELEC Emby Yellow Dragon Repository" version="9.1" provider-name="YLLOW_DRAGON">
    <requires>
        <import addon="xbmc.addon" version="12.0.0"/>
    </requires>
    <extension point="xbmc.addon.repository" name="LibreELEC Emby Yellow Dragon Repository">
        <info compressed="false">https://raw.githubusercontent.com/nkvoronov/repository.yellowdragon.libreelec.emby/9.1/repo/addons.xml</info>
        <checksum>https://raw.githubusercontent.com/nkvoronov/repository.yellowdragon.libreelec.emby/9.1/repo/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/nkvoronov/repository.yellowdragon.libreelec.emby/9.1/repo/</datadir>
        <hashes>false</hashes>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en">YLLOW_DRAGON addons repository for LibreELEC</summary>
        <summary lang="ru">Репозиторий плагинов YLLOW_DRAGON для LibreELEC</summary>
        <description  lang="en">Download and install YLLOW_DRAGON addons repository.</description>
        <description  lang="ru">Загружайте и устанавливайте плагины из репозитория YLLOW_DRAGON.</description>
        <platform>all</platform>
        <assets>
            <icon>resources/icon.png</icon>
            <fanart>resources/fanart.jpg</fanart>
        </assets>
        <news></news>
    </extension>
</addon>

<addon id="service.jellyfin"
       name="Jellyfin Server"
       version="9.1.4"
       provider-name="Team LibreELEC">
  <requires>
    <import addon="os.libreelec.tv" version="9.1"/>
    <import addon="xbmc.python" version="2.1.0"/>

    <import addon="tools.dotnet-runtime" version="0.0.0" />
  </requires>
  <extension point="xbmc.service" library="default.py">
    <provides></provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Jellyfin Server: The Free Software Media System</summary>
    <description>
Jellyfin Server (10.3.7) is the Free Software Media System that puts you in control of managing and streaming your media.There are no strings attached, no premium licenses or features, and no hidden agendas.
    </description>
    <disclaimer>

    </disclaimer>
    <platform>all</platform>
    <news>

    </news>
    <assets>
      <icon>resources/icon.png</icon>
      <fanart>resources/fanart.png</fanart>

    </assets>
  </extension>
</addon>
</addons>
